<ul class="featurehash">
<% featurehash.each_pair do |k,v| %>
    <% unless v.class == Hash %>
        <li><%= link_to "#{k} -- #{v}", "#", {:class => "title_link", :title => "Click for overlay", :'data-rf' => (prefix + [k]).join("."), :'data-spec' => (v.to_s.length < 255 ? v : nil)} %></li>
    <% else %>
		<li><%= k %></li>
        <%= render :partial => 'raw_features', :locals => {:featurehash => v, :prefix => prefix + [k]} %>
    <% end %>
<% end %>
</ul>
