<div id="header">
    <h2>Producing SKUs for category id <%=@category_id%></h2>
</div>
<div id="content">
<div class="strong" id="num_skus_found">
    Total of <%=@product_skus.length%> SKUs found
</div>
<% @product_skus.each do |s| %>
    <div class="skus_to_fetch" data-id=<%=s%>>
        Data for sku <%=s%> still loading
    </div>
<% end %>
</div>
